{
  "extends": [
    "next/core-web-vitals",
    "prettier"
  ],
  "plugins": ["tailwindcss"],
  "rules": {
    "tailwindcss/classnames-order": "warn",
    "tailwindcss/no-custom-classname": "warn",
    "react/no-unescaped-entities": ["error", {
      "forbid": [">", "}"]
    }]
  },
  "settings": {
    "tailwindcss": {
      "config": "./tailwind.config.ts"
    }
  }
}
